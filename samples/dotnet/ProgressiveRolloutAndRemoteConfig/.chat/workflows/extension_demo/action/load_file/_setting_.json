{
	"name": "load_file",
	"description": "Load file content",
	"type": ["file"],
	"action": "load_file",
	"args": [
		{
			"name": "fileName",
			"description": "Specify the file, which content will be loaded",
			"type": "string",
			"from": "content.content.fileName"
		}
	],
	"handler": ["cat", "${fileName}"]
}